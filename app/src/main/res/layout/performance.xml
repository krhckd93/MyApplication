<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"	android:layout_width="match_parent" android:layout_height="wrap_content"	android:background="#F7F7F7"	android:layout_marginBottom="30dp" >	<ScrollView android:id="@+id/Performance" 		android:layout_width="wrap_content" 		android:layout_height="match_parent" >		<RelativeLayout android:id="@+id/content_container" 			android:layout_width="wrap_content" 			android:layout_height="match_parent" >			<RelativeLayout android:id="@+id/frame_5_performance"				android:layout_width="wrap_content"				android:layout_height="wrap_content"				>				<View android:id="@+id/background"					android:background="@drawable/background_shape"					 android:layout_marginStart="0dp"					android:layout_marginTop="0dp"					android:layout_width="1283dp"					android:layout_height="match_parent" />				<ImageView android:id="@+id/image"					android:src="@drawable/image"					android:contentDescription="@string/ek_img_content"					 android:layout_marginStart="0dp"					android:layout_marginTop="194dp"					android:layout_centerHorizontal="true"					android:layout_width="512dp"					android:layout_height="341dp" />				<TextView android:id="@+id/digit"					android:layout_toRightOf="@id/image"					android:fontFamily="OpenSans-Bold"					android:textSize="66sp"					android:textColor="@color/profile__color"					android:layout_marginStart="0dp"					android:layout_marginTop="243dp"					android:layout_width="125dp"					android:layout_height="83dp"					android:text="@string/digit_string"					 />				<RelativeLayout android:id="@+id/top_bar"					android:layout_marginTop="3dp"					android:layout_marginStart="0dp"					android:layout_width="wrap_content"					android:layout_height="wrap_content"					android:background="#FFFFFF"					>					<View android:id="@+id/background_"						android:background="@drawable/background__shape"						 android:layout_marginStart="0dp"						android:layout_marginTop="0dp"						android:layout_width="1286dp"						android:layout_height="103dp" />					<TextView android:id="@+id/performance"						android:fontFamily="OpenSans-Bold"						android:textAllCaps="true"						android:textSize="33sp"						android:textColor="@color/performance_color"						android:layout_marginStart="548dp"						android:layout_marginTop="34dp"						android:layout_width="258dp"						android:layout_height="43dp"						android:text="@string/performance_string"						 />					<ImageView android:id="@+id/back_icon"						android:src="@drawable/back_icon"						android:contentDescription="@string/ek_img_content"						 android:layout_marginStart="44dp"						android:layout_marginTop="26dp"						android:layout_width="41dp"						android:layout_height="41dp" />				</RelativeLayout>			</RelativeLayout>		</RelativeLayout>	</ScrollView></RelativeLayout>