<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"	android:layout_width="match_parent" android:layout_height="wrap_content"	android:background="#FFFFFF"	android:layout_marginBottom="30dp">	<RelativeLayout android:id="@+id/Sector"		android:layout_width="wrap_content" 		android:layout_height="match_parent" >		<RelativeLayout android:id="@+id/content_container" 			android:layout_width="wrap_content" 			android:layout_height="900dp" >			<RelativeLayout android:id="@+id/frame_7_sector_health"				android:layout_width="wrap_content"				android:layout_height="wrap_content"				>				<View android:id="@+id/background"					android:background="@drawable/background_shape"					 android:layout_marginStart="0dp"					android:layout_marginTop="0dp"					android:layout_width="1282dp"					android:layout_height="800dp" />				<ImageView android:id="@+id/image_icon"					android:src="@drawable/image_icon"					android:contentDescription="@string/ek_img_content"					 android:alpha="0.14117647058824"					android:layout_marginStart="0dp"					android:layout_marginTop="375dp"					android:layout_width="292dp"					android:layout_height="324dp" />				<RelativeLayout android:id="@+id/mission"					android:layout_marginTop="148dp"					android:layout_marginStart="0dp"					android:layout_width="wrap_content"					android:layout_height="wrap_content"					>					<TextView android:id="@+id/mission_1"						android:fontFamily="OpenSans-SemiBold"						android:layout_gravity="center_horizontal|center"						android:gravity="center"						android:textSize="33sp"						android:textColor="@color/mission_1_color"						android:layout_marginStart="20dp"						android:layout_marginTop="0dp"						android:layout_width="151dp"						android:layout_height="42dp"						android:text="@string/mission_1_string"						 />				</RelativeLayout>				<TextView android:id="@+id/lorem_ipsum_is_simply_dummy_text_of_the_printing_and_typesettin"					android:fontFamily="OpenSans-SemiBold"					android:textSize="25sp"					android:textColor="@color/lorem_ipsum_is_simply_dummy_text_of_the_printing_and_typesettin_color"					android:layout_marginStart="30dp"					android:layout_marginTop="226dp"					android:layout_width="2768dp"					android:layout_height="200dp"					android:text="@string/lorem_ipsum_is_simply_dummy_text_of_the_printing_and_typesettin_string"					 />				<RelativeLayout android:id="@+id/top_bar"					android:layout_marginTop="2dp"					android:layout_marginStart="0dp"					android:layout_width="wrap_content"					android:layout_height="wrap_content"					>					<View android:id="@+id/background_"						android:background="@drawable/background__shape"						 android:layout_marginStart="0dp"						android:layout_marginTop="0dp"						android:layout_width="1286dp"						android:layout_height="103dp" />					<TextView android:id="@+id/sectors"						android:fontFamily="OpenSans-Bold"						android:textAllCaps="true"						android:textSize="33sp"						android:textColor="@color/mission_1_color"						android:layout_marginStart="558dp"						android:layout_marginTop="34dp"						android:layout_width="156dp"						android:layout_height="43dp"						android:text="@string/sectors_string"						 />					<ImageView android:id="@+id/back_icon"						android:src="@drawable/back_icon"						android:contentDescription="@string/ek_img_content"						 android:layout_marginStart="44dp"						android:layout_marginTop="26dp"						android:layout_width="41dp"						android:layout_height="41dp" />				</RelativeLayout>			</RelativeLayout>		</RelativeLayout>	</RelativeLayout></RelativeLayout>