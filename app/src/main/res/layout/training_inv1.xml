<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"	android:layout_width="match_parent" android:layout_height="wrap_content" >	<RelativeLayout android:id="@+id/first_ui_sv"		android:layout_width="wrap_content" 		android:layout_height="match_parent" >		<RelativeLayout android:id="@+id/content_container" 			android:layout_width="wrap_content" 			android:layout_height="match_parent" >			<RelativeLayout android:id="@+id/first_intervention"				android:layout_width="wrap_content"				android:layout_height="wrap_content"				>				<View android:id="@+id/rectangle_2_copy_2"					android:background="@drawable/rectangle_2_copy_2_shape_tr1"					 android:layout_marginStart="0dp"					android:layout_marginTop="0dp"					android:layout_width="1280dp"					android:layout_height="812dp" />				<ImageView android:id="@+id/capa_1"					android:src="@drawable/capa_1_swipe"					android:contentDescription="@string/ek_img_content"					 android:layout_centerHorizontal="true"					android:layout_marginTop="539dp"					android:layout_width="56dp"					android:layout_height="87dp" />				<RelativeLayout android:id="@+id/ellipse_2"					android:background="@drawable/ellipse_2_shape_tr1"					 android:layout_marginStart="0dp"					android:layout_marginTop="79dp"					android:layout_width="281dp"					android:layout_height="281dp"					android:layout_centerHorizontal="true">					<TextView android:id="@+id/digit"						android:textSize="100sp"						android:textColor="#50AADD"						android:layout_centerHorizontal="true"						android:layout_width="wrap_content"						android:layout_height="wrap_content"						android:text="1"						android:layout_below="@id/intervention"						/>					<TextView android:id="@+id/intervention"						android:textSize="30sp"						android:textColor="@color/intervention_color"						android:layout_marginStart="0dp"						android:layout_marginTop="56dp"						android:layout_width="wrap_content"						android:layout_height="wrap_content"						android:layout_centerHorizontal="true"						android:text="@string/intervention_string"						android:textStyle="bold"						/>				</RelativeLayout>				<TextView android:id="@+id/implement_"					android:layout_gravity="center_horizontal|center"					android:gravity="center"					android:textSize="33sp"					android:textColor="@color/intervention_color"					android:layout_centerHorizontal="true"					android:layout_marginTop="398dp"					android:layout_width="487dp"					android:layout_height="95dp"					android:textStyle="bold"					android:text="@string/implement__string"					 />			</RelativeLayout>		</RelativeLayout>	</RelativeLayout></RelativeLayout>