<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"	android:layout_width="match_parent" android:layout_height="wrap_content" >	<ScrollView android:id="@+id/Sessions" 		android:layout_width="wrap_content" 		android:layout_height="match_parent" >		<RelativeLayout android:id="@+id/content_container" 			android:layout_width="wrap_content" 			android:layout_height="match_parent" >			<RelativeLayout android:id="@+id/frame_6_number_of_sessions"				android:layout_width="wrap_content"				android:layout_height="wrap_content"				>				<View android:id="@+id/background"					android:background="@drawable/background_shape"					 android:layout_marginStart="0dp"					android:layout_marginTop="0dp"					android:layout_width="1283dp"					android:layout_height="700dp" />				<RelativeLayout android:id="@+id/top_bar"					android:layout_marginTop="2dp"					android:layout_marginStart="0dp"					android:layout_width="wrap_content"					android:layout_height="wrap_content"					>					<View android:id="@+id/background_"						android:background="@drawable/background__shape"						 android:layout_marginStart="0dp"						android:layout_marginTop="0dp"						android:layout_width="1286dp"						android:layout_height="103dp" />					<TextView android:id="@+id/sessions"						android:fontFamily="OpenSans-Bold"						android:textAllCaps="true"						android:textSize="33sp"						android:textColor="@color/sessions_color"						android:layout_marginStart="558dp"						android:layout_marginTop="34dp"						android:layout_width="168dp"						android:layout_height="43dp"						android:text="Sessions"						android:textStyle="bold"						 />					<ImageView android:id="@+id/back_icon"						android:src="@drawable/back_icon"						android:contentDescription="@string/ek_img_content"						 android:layout_marginStart="44dp"						android:layout_marginTop="26dp"						android:layout_width="41dp"						android:layout_height="41dp" />				</RelativeLayout>				<RelativeLayout android:id="@+id/description"					android:layout_marginTop="0dp"					android:layout_marginStart="0dp"					android:layout_width="wrap_content"					android:layout_height="wrap_content"					>				</RelativeLayout>				<ImageView					android:layout_width="wrap_content"					android:layout_height="wrap_content"					android:src="@drawable/sessions_completed"					android:layout_marginTop="194dp"					android:layout_marginStart="200dp"/>				<ImageView					android:layout_width="wrap_content"					android:layout_height="wrap_content"					android:src="@drawable/sessions_streak"					android:layout_marginTop="194dp"					android:layout_marginEnd="200dp"					android:layout_alignParentRight="true"					/>			</RelativeLayout>		</RelativeLayout>	</ScrollView></RelativeLayout>